<AssignmentRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CaseAssignmentRule</fullName>
    <ruleEntry>
        <assignedTo>
            <queue>LiveChatCasesQueue</queue>
        </assignedTo>
        <booleanFilter>1</booleanFilter>
        <criteriaItems>
            <field>Case.Origin</field>
            <operation>equals</operation>
