        </criteriaItems>
        <sortOrder>1</sortOrder>
    </ruleEntry>
    <ruleEntry>
        <assignedTo>
            <queue>EmailCasesQueue</queue>
        </assignedTo>
        <booleanFilter>1</booleanFilter>
        <criteriaItems>
            <field>Case.Origin</field>
            <operation>equals</operation>
            <value>Email</value>
        </criteriaItems>
        <sortOrder>2</sortOrder>
    </ruleEntry>
</AssignmentRule>