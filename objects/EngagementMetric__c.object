            <picklistValues>
                <fullName>Email Open</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Click-Through</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Social Media Interaction</fullName>
                <default>false</default>
            </picklistValues>
        </picklist>
    </fields>
    <fields>
        <fullName>Value__c</fullName>
        <externalId>false</externalId>
        <label>Value</label>
        <type>Number</type>
        <precision>18</precision>
        <scale>2</scale>
    </fields>
    <fields>
        <fullName>Timestamp__c</fullName>
        <externalId>false</externalId>
        <label>Timestamp</label>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>RelatedCampaign__c</fullName>
        <externalId>false</externalId>
        <label>Related Campaign</label>
        <type>Lookup</type>
        <referenceTo>Campaign</referenceTo>
    </fields>
