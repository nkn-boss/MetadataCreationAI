            <picklistValues>
                <fullName>Draft</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Reviewed</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Final</fullName>
                <default>true</default>
