<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
  <fullName>Automate_Lead_Qualification_Flow</fullName>
  <label>Automate Lead Qualification Flow</label>
  <processType>Workflow</processType>
  <status>Active</status>
  <startElementReference>Start</startElementReference>
  <elements>
    <element>
      <name>Update_Qualification_Status</name>
      <type>RecordUpdate</type>
      <recordUpdate>
        <criteria>
          <field>Lead_Score__c</field>
          <operator>greaterThan</operator>
          <value>50</value>
        </criteria>
        <criteria>
          <field>Engagement_Level__c</field>
          <operator>equals</operator>
          <value>High</value>
        </criteria>
        <action>
          <field>Qualification_Status__c</field>
          <value>Qualified</value>
        </action>
      </recordUpdate>
    </element>
    <element>
      <name>Notify_Sales_Rep</name>
      <type>EmailAlert</type>
      <emailAlert>
        <recipients>
          <recipientType>Owner</recipientType>
        </recipients>
        <template>Lead_Qualified_Notification</template>
      </emailAlert>
    </element>
  </elements>
</Flow>