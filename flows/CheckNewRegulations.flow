<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <processType>Flow</processType>
    <status>Active</status>
    <startElementReference>Start</startElementReference>
    <elements>
        <element>
            <name>Start</name>
            <type>Start</type>
            <schedule>
                <frequency>Daily</frequency>
                <startDate>2023-01-01</startDate>
            </schedule>
        </element>
        <element>
            <name>CheckRegulations</name>
            <type>ActionCall</type>
            <actionName>RegulationService.checkNewRegulations</actionName>
        </element>
        <element>
            <name>NotifyAdmin</name>
            <type>ActionCall</type>
            <actionName>NotificationService.sendNotification</actionName>
            <inputParameters>
                <parameter>
                    <name>message</name>
                    <value>New regulations have been identified.</value>
                </parameter>
            </inputParameters>
        </element>
    </elements>
    <connectors>
        <connector>
            <sourceReference>Start</sourceReference>
            <targetReference>CheckRegulations</targetReference>
        </connector>
        <connector>
            <sourceReference>CheckRegulations</sourceReference>
            <targetReference>NotifyAdmin</targetReference>
        </connector>
    </connectors>
